{
    "name": "nobility-loi-generator",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "rollup -c --watch",
        "build": "rollup -c",
        "lint": "eslint \"src/**/*.js\"",
        "format": "prettier --write \"src/**/*.{js,css,html,json}\"",
        "test": "jest",
        "prepare": "husky install"
    },
    "lint-staged": {
        "src/**/*.{js,css,html,json}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/preset-env": "^7.27.1",
        "@rollup/plugin-commonjs": "^23.0.0",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-terser": "^0.4.4",
        "babel-jest": "^29.7.0",
        "eslint": "^8.0.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^13.0.0",
        "prettier": "^3.0.0",
        "rollup": "^3.0.0"
    }
}
